{
    "deploy-project": {
        "project": "project-name",
        "box": "box-name",
        "box_url": "box-url",
        "memory": 256,
        "ip": "10.2.2.15",
        "source": "../",
        "path": "/var/www/project-name",
        "domain": "project-name.local",
        "aliases": ["www.project-name.local"],
        "type": "type-project",
        "repo": {
            "url": "url-repository",
            "branch": "master",
            "private_key": "private_key",
            "public_key": "public_key",
            "erase_path": false,
            "method": "sync"
        },
        "db" : {
            "": "db-name",
            "host": "127.0.0.1",
            "user": "root",
            "password": "",
            "install": "install.sql",
            "provider": "mysql",
            "migrate": "command/for/migrate",
            "migrate_cwd": "current/directory/for/migrate/command",
            "install_type": "none"
        },
        "disabled": [
            "apache-configure"
        ],
        "php-oc": {
            "redirect": "",
            "base": "/",
            "htaccess": {
                "template": "template.erb",
                "cookbook": "cookbook"
            },
            "informations": [
                {
                    "title": "Title",
                    "keyword": "slug",
                    "template": "path/to/template.lang.tpl",
                    "sort_order": 0,
                    "bottom": 1
                }
            ],
            "settings": {
                "group": [{
                    "key": "value"
                }]
            },
            "permissions": {
                "access": {
                    "route": ["Group 1", "Group 2"],
                    "route_2": {
                        "added": ["Group 1", "Group 2"],
                        "removed": ["Group 3", "Group 4"]
                    }
                },
                "modify": {

                }
            },
            "modules": {
                "module_1": "install",
                "module_2": "uninstall"
            },
            "payments": {
                "payment_1": "install",
                "payment_2": "uninstall"
            }
        },
        "php-cake": {
            "redirect": "",
            "base": "/"
        },
        "backup": {
            "path": "/path/to/backup",
            "bin": "/path/to/binary",
            "log": "/path/to/log"
        }
    }
}